generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model universoficcional {
  id        Int      @id @default(autoincrement())
  nome    String   @unique
  criador     String?
  midia String 
  genero     String
  cronologia DateTime
  personagens   String
  popularidade  Float
  ativo   Boolean
  @@map("universoficcional")
}
